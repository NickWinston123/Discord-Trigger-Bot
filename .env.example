DISCORD_TOKEN=your_discord_bot_token_here

# Shared secret sent in webhook headers for auth (you validate this in your automation tool)
DASHCORD_SHARED_SECRET=my_super_secret_string
DASHCORD_DEBUG=1

# Prefix
COMMAND_PREFIX=!

# Timezone metadata
TIMEZONE=America/New_York

DISPLAY_UNKNOWN_COMMAND_ERROR=true

VERIFY_TLS=false
DEBUG_WEBHOOK=1
PYTHONUNBUFFERED=1


# panel behavior
PANEL_REPOST_ON_STARTUP=true
PANEL_REPOST_ON_CLICK=false

# cleanup old panel messages (recommended)
PANEL_DELETE_OLD_PANELS=false
PANEL_SCAN_LIMIT=50
PANEL_STATUS_LINE=true
PANEL_SPAWN_NEW_ON_CLICK=true
PANEL_ARCHIVE_DISABLE_BUTTONS=true
PANEL_FORCE_NEW_ON_STARTUP=false

PANEL_PERSIST_DEFAULT=true
PANEL_PERSIST_INTERVAL_SECONDS=120
PANEL_PERSIST_CLEANUP_OLD_ACTIVE=true

HTTP_TIMEOUT_SECONDS=120